{
  "name": "carbon-react-color-theme",
  "displayName": "Carbon React Color Theme",
  "description": "Carbon React Color Theme",
  "version": "1.2.1",
  "publisher": "yathink3",
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "images/icon.png",
  "contributes": {
    "configurationDefaults": {
      "terminal.integrated.defaultProfile.windows": "Command Prompt",
      "terminal.integrated.defaultProfile.linux": null,
      "terminal.integrated.defaultProfile.osx": null,
      "editor.fontLigatures": true,
      "editor.linkedEditing": true,
      "editor.cursorBlinking": "expand",
      "editor.bracketPairColorization.enabled": true,
      "editor.suggestSelection": "first",
      "editor.suggest.preview": true,
      "editor.smoothScrolling": true,
      "workbench.list.smoothScrolling": true,
      "editor.cursorSmoothCaretAnimation": "on",
      "editor.inlineSuggest.enabled": true,
      "editor.snippetSuggestions": "top",
      "editor.glyphMargin": true,
      "editor.minimap.enabled": false,
      "files.trimTrailingWhitespace": true,
      "files.trimFinalNewlines": true,
      "terminal.integrated.macOptionIsMeta": true,
      "diffEditor.ignoreTrimWhitespace": false,
      "emmet.triggerExpansionOnTab": true,
      "editor.accessibilitySupport": "on"
    },
    "themes": [
      {
        "label": "Carbon React Color Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/CarbonReact-color-theme.json"
      },
      {
        "label": "Carbon React Color Theme - Pure",
        "uiTheme": "vs-dark",
        "path": "./themes/CarbonReact-pure-theme.json"
      },
      {
        "label": "Carbon React Color Theme - Winter",
        "uiTheme": "vs-dark",
        "path": "./themes/CarbonReact-winter-theme.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yathink3/carbon-react-color-theme.git"
  },
  "homepage": "https://github.com/yathink3/carbon-react-color-theme/blob/master/README.md",
  "keywords": [
    "vscode",
    "theme",
    "onedark",
    "monokai",
    "darker",
    "carbon",
    "react",
    "winter"
  ],
  "author": "Eser Ozvataf <eser@ozvataf.com> (http://eser.ozvataf.com)",
  "license": "MIT"
}
