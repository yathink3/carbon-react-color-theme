{
  "name": "carbon-react-color-theme",
  "displayName": "Carbon React Color Theme",
  "description": "Carbon React Color Theme",
  "version": "1.1.5",
  "publisher": "yathink3",
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Themes"
  ],
  "icon": "images/icon.png",
  "contributes": {
    "configurationDefaults": {
      "editor.fontSize": 13.2,
      "editor.lineHeight": 20,
      "terminal.integrated.lineHeight": 1.1,
      "terminal.integrated.fontSize": 13,
      "editor.fontLigatures": true,
      "editor.linkedEditing": true,
      "editor.cursorBlinking": "expand",
      "editor.bracketPairColorization.enabled": true,
      "editor.suggestSelection": "first",
      "editor.suggest.preview": true,
      "editor.smoothScrolling": true,
      "workbench.list.smoothScrolling": true,
      "editor.cursorSmoothCaretAnimation": "on",
      "editor.inlineSuggest.enabled": true,
      "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
      "terminal.integrated.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
      "editor.snippetSuggestions": "top",
      "editor.glyphMargin": true,
      "editor.minimap.enabled": false,
      "files.trimTrailingWhitespace": true,
      "files.trimFinalNewlines": true,
      "terminal.integrated.macOptionIsMeta": true,
      "diffEditor.ignoreTrimWhitespace": false,
      "emmet.triggerExpansionOnTab": true,
      "editor.accessibilitySupport": "on"
    },
    "themes": [
      {
        "label": "Carbon React Color Theme",
        "uiTheme": "vs-dark",
        "path": "./themes/CarbonReact-color-theme.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yathink3/carbon-react-color-theme.git"
  },
  "homepage": "https://github.com/yathink3/carbon-react-color-theme/blob/master/README.md",
  "keywords": [
    "vscode",
    "theme",
    "onedark",
    "monokai",
    "darker",
    "carbon",
    "react"
  ],
  "author": "Eser Ozvataf <eser@ozvataf.com> (http://eser.ozvataf.com)",
  "license": "MIT"
}
